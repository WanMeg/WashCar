/* pages/my/my.wxss */


.header_view {
  background-color: #50b5eb;
  width: 100%;
  height: 160rpx;
  display: flex;
  align-items: center;
}

.header_image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-left: 30rpx;
}


.phone {
  margin-left: 25rpx;
  color: white;
  font-size: 28rpx;
}


.list_view {
  margin-left: 30rpx;
  margin-right: 30rpx;
  height: 90rpx;
  border-bottom: 0.4rpx solid #f1f2f4;
  display: flex;
  align-items: center;
}


.list_icon {
  width: 40rpx;
  height: 40rpx;
}

.list_text {
  margin-left: 25rpx;
  font-size: 28rpx;
  color: #333;
}